    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "http-status": "^2.0.0",
    "prisma": "^6.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
